 <!-- Post preview-->
 <% postData.forEach(post =>{ %>
  <% console.log(post) %>
  <div class="post-preview">
    <a href="post">
      <h2 class="post-title">
        <%= post.postTitle %>
      </h2>
      <h3 class="post-subtitle">
        <%= post.postSubtitle %>
      </h3>
    </a>
    <p class="post-meta">
      Posted by
      <a href="#!">Start Bootstrap</a>
      on <%= post.postDate %>
    </p>
  </div>
  <hr class="my-4" />
 <%})%>
 